<h1>Productos</h1>
<div id= "products">
    {{#if products}}
        <ul>
        {{#each products}}
            <li>
                <h3>Title:</h3> {{this.title}}
                <h4>price:</h4> {{this.price}}
            </li>
        {{/each}}
        </ul>
        {{else}}
        <p>No hay productos</p>
    {{/if}}
</div>